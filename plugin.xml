<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="CCFINDERX.commands.category"
            name="CCFINDERX">
      </category>
      <command
            categoryId="CCFINDERX.commands.category"
            id="CCFINDERX.commands.sampleCommand"
            name="Sample Command">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="ccfinderx.handlers.ExecutionHandler"
            commandId="CCFINDERX.commands.sampleCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="CCFINDERX.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="CCFINDERX.menus.sampleMenu"
               label="CCFINDERX"
               mnemonic="M">
            <command
                  commandId="CCFINDERX.commands.sampleCommand"
                  id="CCFINDERX.menus.sampleCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="CCFINDERX.toolbars.sampleToolbar">
            <command
                  commandId="CCFINDERX.commands.sampleCommand"
                  icon="icons/sample.gif"
                  id="CCFINDERX.toolbars.sampleCommand"
                  tooltip="Say hello world">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="CCFINDERX"
            name="CCFINDERX">
      </category>
      <view
            category="CCFINDERX"
            class="ccfinderx.ui.views.CloneSetView"
            icon="icons/sample.gif"
            id="ccfinderx.ui.views.CloneSetView"
            name="Clone-Set View">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="ccfinderx.ui.views.CloneSetView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
